					},
					{
						"name": "ExtendedPrice",
						"type": "decimal",
						"precision": 18,
						"scale": 2
					},
					{
						"name": "LastEditedBy",
						"type": "int",
						"precision": 10
					},
					{
						"name": "LastEditedWhen",
						"type": "datetime2",
						"scale": 7
					}
				],
				"typeProperties": {
					"schema": "Sales",
					"table": "InvoiceLines"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/wwi_OLTP')]"
			]
